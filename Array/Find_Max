
class Main {
    public static void main(String[] args) {
        
        int []arr = {1,55,8,7,6,377,5,4,9};
        
        //Max
        int max = 0;
        for (int i=0;i<arr.length;i++){
            if(arr[i] > max){
                max = arr[i];
                
            }
            
     }
    System.out.println(max);   
        
    }
}
