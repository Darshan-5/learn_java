
class Main {
    public static void main(String[] args) {
        
        int []arr = {1,55,8,7,6,377,5,4,9};
        
        //Max
        int sum = 0;
        // for (int i=0;i<arr.length;i++){
        for (int i : arr){
                sum += i;
        }
     System.out.println(sum);         
     }
     
        
    }
