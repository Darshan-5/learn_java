          

class Main {
    public static void main(String[] args) {
        //array initialize
        int []arr = new int[10];
        arr[5]= 5;
        arr[8]= 5;
        arr[9]= 5;
        // acess
        
        //for each
        for (int i : arr){
            
            System.out.println(i);
        }
        
        
        
    }
}
